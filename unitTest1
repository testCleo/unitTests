public class Sample {
	
		
	
    public static void main( String[] args ) {

    	Sample addition = new Sample();
    	addition.add(10, 5);
    	System.out.println("---------Update 1---------");
    	System.out.println("---------Update 3---------");
    	
    }

	public int add(int x,int y) {
		
		int z = x + y;
	    System.out.println("Sum of entered integers = "+z);
		return z;
	}
}
